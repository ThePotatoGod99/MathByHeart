<formulaList>
    <formula>
        <question>$$L$$</question>
        <answer>$$\int_a^b\sqrt{1+(f'(x))^2}dx$$</answer>
        <starred>false</starred>
    </formula>

    <formula>
        <question>$$\int_a^bf(x)dx$$</question>
        <answer>$$\sum_{i=1}^n f(a+i\frac{(b-a)}{n})*\frac{b-a}{n}$$</answer>
        <starred>false</starred>
    </formula>

    <formula>
        <question>$$\int_a^bf(x)dx$$ (if discontinuous at c)</question>
        <answer>$$\int_a^cf(x)dx+\int_c^bf(x)dx$$</answer>
        <starred>false</starred>
    </formula>

    <formula>
